{
  "name": "spyglass-cli",
  "version": "0.5.1",
  "description": "Spyglass command-line interface",
  "author": "Spyglass Software, Inc.",
  "bin": {
    "spyglass": "./bin/run"
  },
  "homepage": "https://github.com/spyglasshq/spyglass-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "spyglasshq/spyglass-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "@iarna/toml": "^2.2.5",
    "@oclif/color": "^1.0.13",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "axios": "^1.6.0",
    "deep-object-diff": "^1.1.9",
    "fs-extra": "^11.1.1",
    "isomorphic-git": "^1.24.5",
    "snowflake-sdk": "^1.9.0",
    "winston": "^3.11.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.6",
    "@types/chai": "^4",
    "@types/chai-as-promised": "^7.1.7",
    "@types/fs-extra": "^11.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.60",
    "@types/snowflake-sdk": "^1.6.16",
    "@types/supports-color": "^8.1.2",
    "chai": "^4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "spyglass",
    "dirname": "spyglass",
    "commands": "./dist/commands",
    "topics": {
      "accounts": {
        "description": "Manage accounts."
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "software.spyglass.cli"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:unit": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:integration": "mocha --forbid-only \"integration-test/**/*.test.ts\"",
    "coverage": "npx nyc --reporter=lcov --reporter=text npm test",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "npx nyc --silent npm run test:unit",
    "cover:integration": "npx nyc --silent --no-clean npm run test:integration",
    "cover:report": "npx nyc report --reporter=lcov --reporter=text"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/spyglasshq/spyglass-cli/issues",
  "keywords": [
    "security",
    "iam",
    "snowflake",
    "access-control",
    "access-management",
    "database-security",
    "spyglass"
  ],
  "types": "dist/index.d.ts"
}
